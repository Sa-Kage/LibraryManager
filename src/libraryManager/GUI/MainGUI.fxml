                        </TableView>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab disable="true" text="%MainGUI.Movies">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
